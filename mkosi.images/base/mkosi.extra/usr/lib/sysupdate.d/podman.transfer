[Transfer]
Features=podman
Verify=false

[Source]
Type=url-file
Path=https://repository.frostyard.org/ext/podman/
MatchPattern=podman_@v_@a.raw.zst \
             podman_@v_@a.raw.xz \
             podman_@v_@a.raw.gz \
             podman_@v_@a.raw

[Target]
Type=regular-file
Path=/var/lib/extensions.d/
MatchPattern=podman_@v_@a.raw.zst \
             podman_@v_@a.raw.xz \
             podman_@v_@a.raw.gz \
             podman_@v_@a.raw
CurrentSymlink=podman.raw
